<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dungzi.villakorea" version="3" android-versionCode="3" xmlns="http://www.w3.org/ns/widgets"  xmlns:gap="http://phonegap.com/ns/1.0">
    <name>좋은집구하기연구소</name>
    <description>	
        신축빌라분양,임대,매매,부동산앱
    </description>
    <author email="webplug@gmail.com" href="http://www.dungzi.com/">
        Dungzi Team
    </author>
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="SplashScreenDelay" value="10000" />
	<preference name="android-minSdkVersion" value="15" />
	<preference name="android-targetSdkVersion" value="26" />
	<preference name="fullscreen" value="true" />
	
	<gap:plugin name="cordova-plugin-whitelist" source="npm" />
	<gap:plugin name="cordova-plugin-inappbrowser" source="npm"  />
	
	<allow-navigation href="http://villakorea.dungzi.com/*" />
	<icon src="icon.png" />
	<content src="index.html" />
	<access origin="*" />
</widget>